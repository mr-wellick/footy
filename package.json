{
  "name": "footy",
  "type": "module",
  "scripts": {
    "serve": "vite",
    "build:css": "npx tailwindcss -i ./src/public/input.css -o ./src/public/output.css --watch",
    "ui:build": "vite build --watch",
    "dev": "concurrently \"npm run serve\" \"npm run build:css\" \"npm run ui:build\"",
    "test": "vitest"
  },
  "dependencies": {
    "@hono/node-server": "^1.13.1",
    "@hono/swagger-ui": "^0.4.1",
    "@hono/zod-validator": "^0.2.2",
    "@prisma/client": "^5.19.1",
    "hono": "^4.6.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "concurrently": "^9.0.1",
    "daisyui": "^4.12.10",
    "prisma": "^5.19.1",
    "tailwindcss": "^3.4.11",
    "vite": "^5.4.6",
    "vitest": "^2.1.1"
  }
}
